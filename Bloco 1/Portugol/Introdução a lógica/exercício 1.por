programa
{
/*
1. Faça um sistema que leia a idade de uma pessoa expressa em anos, meses e
dias e mostre-a expressa apenas em dias.	
 */
	funcao inicio()
	{
      inteiro ano = 365 
      inteiro idade 
      inteiro mes 
      
		escreva("\nPor favor, sua idade?")
		leia(idade)

		escreva("\nQual seu mes?")
		leia(mes)

		escreva("\nTotal de dias de vida:", ano* idade)
		
		

       
	}
}

